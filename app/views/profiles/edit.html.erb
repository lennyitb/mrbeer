<h1>Mr. Beer</h1>
<h2>Edit Profile</h2>
<%= form_with model: Current.user, url: edit_profile_path do |f| %>
	<p>
		<%= f.label 'username:' %><br>
		<%= f.text_field :username %>
	</p>
	<p>
		<%= f.label 'first name' %><br>
		<%= f.text_field :nickname %>
	</p>
	<p>
		<%= f.submit 'Update' %>
	</p>
<% end %>
<p><a href="/">Go Back</a></p>